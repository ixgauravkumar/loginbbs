
docker run -itd -d \
  --name flask-app25 \
  --add-host=host.docker.internal:host-gateway \
  -e DB_USER=flaskuser \
  -e DB_PASSWORD=flaskpass \
  -e DB_HOST=host.docker.internal \
  -e DB_PORT=3306 \
  -e DB_NAME=login_db \
  -e MAIL_USERNAME=@@@@@@@@@@@@@@@@@@\
  -e MAIL_PASSWORD=################## \
  -e ADMIN_EMAIL=@@@@@@@@@@@@@@@@@@@@ \
  -e FLASK_SECRET_KEY=############### \
  -p 8084:5000 \
